<header class="header">
  <nav class="blog-menu">
    <% for(name in theme.menu) { %>
      <div class="blog-menu-item">
        <a href="<%- url_for(theme.menu[name]) %>" class="blog-menu-item-link"><%=name%></a>
      </div>
    <% } %>
  </nav>
  <div class="blog-header">
    <div class="blog-title">
      <a href="<%- url_for() %>" class="logo"><%=config.title%></a>
    </div>
  </div>
</header>